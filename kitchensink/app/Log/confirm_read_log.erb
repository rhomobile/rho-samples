<div data-role="page">

  <%= render :partial => "/partials/page_head" %>
    
  <div data-role="content">
  
    <%= render :partial => "/partials/example_header", :locals => {:title => "Read log file", :description => "
      This example will read part of the log file and show it
    " }
    %>

    <%= render :partial => "/partials/language_tabs" %>
    
    <div class="sample ruby">
        <a href="<%= url_for :action => :read_log_file %>" data-role="button">Read log file</a>
    </div>
    
    <div class="sample javascript">
        <a href="#" data-role="button" onclick="KitchenSink.Samples.Log.read_log_file()">Read log file</a>
    </div>
    
    
  </div>
</div>